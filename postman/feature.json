      "item": [
        {
          "name": "PATCH /admin/events/{eventId} - moderation note happy path",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const main=async()=>{",
                  "const b=pm.collectionVariables.get('baseUrl');",
                  "const q=(r)=>new Promise((ok,er)=>pm.sendRequest(r,(e,s)=>e?er(e):ok(s)));",
                  "const s=${Date.now()}_;",
                  "const d=new Date(Date.now()+6*60*60*1000); const p=n=>String(n).padStart(2,'0');",
                  "const dt=${d.getFullYear()}-- ::;",
                  "const u=await q({url:${b}/admin/users,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({name:ModInit ,email:mod_init_@mail.com})}}); if(u.code!==201) throw new Error('u');",
                  "const cat=await q({url:${b}/admin/categories,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({name:ModCat })}}); if(cat.code!==201) throw new Error('cat');",
                  "const ev=await q({url:${b}/users//events,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({title:ModEv ,annotation:'Moderation annotation text long enough.',description:'Moderation description text long enough for validation checks.',category:cat.json().id,eventDate:dt,location:{lat:48.85,lon:2.35},paid:false,participantLimit:20,requestModeration:true})}}); if(ev.code!==201) throw new Error('event');",
                  "pm.collectionVariables.set('moderationEventId',ev.json().id);",
                  "};",
                  "main().catch(e=>{throw e;});"
                ]
              }
            },
