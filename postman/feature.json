    {
      "name": "Rating Events",
      "item": [
        {
          "name": "PUT /users/{userId}/events/{eventId}/rating - happy path",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const main=async()=>{",
                  "const b=pm.collectionVariables.get('baseUrl');",
                  "const q=(r)=>new Promise((ok,er)=>pm.sendRequest(r,(e,s)=>e?er(e):ok(s)));",
                  "const s=${Date.now()}_;",
                  "const d=new Date(Date.now()+6*60*60*1000); const p=n=>String(n).padStart(2,'0');",
                  "const dt=${d.getFullYear()}-- ::;",
                  "const mk=async(n,e)=>q({url:${b}/admin/users,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({name:n,email:e})}});",
                  "const u1=await mk(RateInit ,\rate_init_@mail.com); if(u1.code!==201) throw new Error('u1');",
                  "const u2=await mk(RateUser ,\rate_user_@mail.com); if(u2.code!==201) throw new Error('u2');",
                  "const cat=await q({url:${b}/admin/categories,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({name:RateCat })}}); if(cat.code!==201) throw new Error('cat');",
                  "const ev=await q({url:${b}/users//events,method:'POST',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({title:RateEv ,annotation:'Rating annotation text long enough.',description:'Rating description text long enough for validation checks.',category:cat.json().id,eventDate:dt,location:{lat:55.75,lon:37.61},paid:false,participantLimit:10,requestModeration:true})}}); if(ev.code!==201) throw new Error('event');",
                  "const eid=ev.json().id;",
                  "const pub=await q({url:${b}/admin/events/,method:'PATCH',header:{'Content-Type':'application/json'},body:{mode:'raw',raw:JSON.stringify({stateAction:'PUBLISH_EVENT',moderationNote:'Publish for rating'})}}); if(pub.code!==200) throw new Error('publish');",
                  "pm.collectionVariables.set('ratingUserId',u2.json().id);",
